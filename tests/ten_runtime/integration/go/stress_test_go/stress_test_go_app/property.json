{
  "ten": {
    "log": {
      "handlers": [
        {
          "matchers": [
            {
              "level": "debug"
            }
          ],
          "formatter": {
            "type": "plain",
            "colored": true
          },
          "emitter": {
            "type": "console",
            "config": {
              "stream": "stdout"
            }
          }
        }
      ]
    },
    "predefined_graphs": [
      {
        "name": "default",
        "auto_start": true,
        "graph": {
          "nodes": [
            {
              "type": "extension",
              "name": "simple_http_server_cpp",
              "addon": "simple_http_server_cpp",
              "extension_group": "default_extension_group",
              "property": {
                "server_port": 8002
              }
            },
            {
              "type": "extension",
              "name": "server1",
              "addon": "default_extension_go",
              "extension_group": "server1",
              "property": {
                "return_ok": true,
                "response_delay": 0
              }
            },
            {
              "type": "extension",
              "name": "server2",
              "addon": "default_extension_go",
              "extension_group": "server2",
              "property": {
                "return_ok": false,
                "response_delay": 0
              }
            },
            {
              "type": "extension",
              "name": "server3",
              "addon": "default_extension_go",
              "extension_group": "server3",
              "property": {
                "return_ok": true,
                "response_delay": 0
              }
            },
            {
              "type": "extension",
              "name": "client",
              "addon": "default_extension_go",
              "extension_group": "client",
              "property": {
                "enable_stress_test": true,
                "burst_count": 500,
                "burst_interval_ms": 10,
                "total_bursts": 200,
                "force_gc": true,
                "concurrent_sends": 5
              }
            }
          ],
          "connections": [
            {
              "extension": "simple_http_server_cpp",
              "cmd": [
                {
                  "name": "test",
                  "dest": [
                    {
                      "extension": "client"
                    }
                  ]
                }
              ]
            },
            {
              "extension": "client",
              "cmd": [
                {
                  "name": "test",
                  "dest": [
                    {
                      "extension": "server1"
                    },
                    {
                      "extension": "server2"
                    },
                    {
                      "extension": "server3"
                    }
                  ]
                },
                {
                  "name": "test_stress",
                  "dest": [
                    {
                      "extension": "server1"
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    ]
  }
}