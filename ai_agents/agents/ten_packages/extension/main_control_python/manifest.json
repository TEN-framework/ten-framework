{
  "type": "extension",
  "name": "main_control_python",
  "version": "0.1.0",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.10"
    },
    {
      "type": "system",
      "name": "ten_ai_base",
      "version": "=0.6.9"
    }
  ],
  "package": {
    "include": [
      "manifest.json",
      "property.json",
      "**.tent",
      "**.py",
      "README.md",
      "tests/**"
    ]
  },
  "api": {
    "data_in": [
      {
        "name": "asr_result",
        "property": {
          "properties": {
            "text": {
              "type": "string"
            },
            "final": {
              "type": "bool"
            },
            "metadata": {
              "type": "object",
              "properties": {
                "session_id": {
                  "type": "string"
                }
              }
            }
          }
        }
      },
      {
        "name": "text_data",
        "property": {
          "properties": {
            "text": {
              "type": "string"
            },
            "is_final": {
              "type": "bool"
            },
            "stream_id": {
              "type": "uint32"
            }
          }
        }
      }
    ],
    "data_out": [
      {
        "name": "text_data",
        "property": {
          "properties": {
            "text": {
              "type": "string"
            },
            "is_final": {
              "type": "bool"
            },
            "stream_id": {
              "type": "uint32"
            }
          }
        }
      },
      {
        "name": "tts_request",
        "property": {
          "properties": {
            "text": {
              "type": "string"
            }
          }
        }
      }
    ],
    "cmd_in": [
      {
        "name": "on_user_joined"
      },
      {
        "name": "on_user_left"
      }
    ],
    "cmd_out": [
      {
        "name": "flush_llm"
      },
      {
        "name": "flush_tts"
      },
      {
        "name": "flush_rtc"
      }
    ]
  }
}