{
  "type": "extension",
  "name": "transcribe_asr_python",
  "version": "0.1.0",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.10"
    },
    {
      "path": "/tmp/ten_ai_base"
    }
  ],
  "api": {
    "interface": [{
      "import_uri": "../../system/ten_ai_base/api/asr-interface.json"
    }],
    "property": {
      "properties": {
        "region": {
          "type": "string"
        },
        "access_key": {
          "type": "string"
        },
        "secret_key": {
          "type": "string"
        },
        "sample_rate": {
          "type": "int64"
        },
        "lang_code": {
          "type": "string"
        }
      }
    }
  }
}