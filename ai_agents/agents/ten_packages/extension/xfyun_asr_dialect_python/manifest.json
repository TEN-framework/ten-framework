{
    "type": "extension",
    "name": "xfyun_asr_dialect_python",
    "version": "0.1.2",
    "dependencies": [
        {
            "type": "system",
            "name": "ten_runtime_python",
            "version": "0.10"
        },
        {
            "type": "system",
            "name": "ten_ai_base",
            "version": "0.6"
        }
    ],
    "api": {
        "interface": [
            {
                "import_uri": "../../system/ten_ai_base/api/asr-interface.json"
            }
        ],
        "property": {
            "properties": {
                "app_id": {
                    "type": "string"
                },
                "access_key_id": {
                    "type": "string"
                },
                "access_key_secret": {
                    "type": "string"
                },
                "language": {
                    "type": "string"
                },
                "samplerate": {
                    "type": "int64"
                }
            }
        }
    },
    "package": {
        "include": [
            "manifest.json",
            "property.json",
            "**.py",
            "requirements.txt",
            "docs/**"
        ]
    }
}