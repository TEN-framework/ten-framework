{
    "type": "extension",
    "name": "xfyun_asr_python",
    "version": "0.1.1",
    "dependencies": [
        {
            "type": "system",
            "name": "ten_runtime_python",
            "version": "0.10"
        },
        {
            "type": "system",
            "name": "ten_ai_base",
            "version": "0.6"
        }
    ],
    "api": {
        "interface": [
            {
                "import_uri": "../../system/ten_ai_base/api/asr-interface.json"
            }
        ],
        "property": {
            "properties": {
                "app_id": {
                    "type": "string"
                },
                "api_key": {
                    "type": "string"
                },
                "api_secret": {
                    "type": "string"
                },
                "lang": {
                    "type": "string"
                },
                "sample_rate": {
                    "type": "int64"
                }
            }
        }
    }
}